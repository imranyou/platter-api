
CREATE TABLE USER (
    USER_ID int,
    FIRST_NAME varchar(255),
    LAST_NAME varchar(255),
    EMAIL varchar(255),
    IMG_URL varchar(255),
    SPACES varchar(255)
);


CREATE TABLE IDEA (
    ID int,
    TITLE varchar(255),
    DESCRIPTION varchar(255),
    USER varchar(255)
);



